{
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "node",
    ],
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": false,
    "outDir": "dist",
    "paths": {
      "*": [
        "node_modules/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts"
  ]
}